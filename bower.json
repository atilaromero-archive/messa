{
  "name": "messa",
  "version": "0.0.1",
  "authors": [
    "Simone Busoli <simone.busoli@gmail.com>"
  ],
  "description": "messa is an ExpressJS app to scaffold a CRUD UI for your mongoose models built with angular.js",
  "license": "MIT",
  "keywords": [
    "express",
    "mongoose",
    "scaffold",
    "crud",
    "angular"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-material": "~0.10.0",
    "angular-ui-grid": "~3.0.1",
    "lodash": "~3.10.0",
    "sugar": "~1.4.1",
    "jquery": "~2.1.4",
    "angular-messages": "1.3.16"
  },
  "resolutions": {
    "angular": ">=1.2.16 <=1.3.16"
  }
}
